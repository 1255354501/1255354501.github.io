
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.text-area {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	padding-left:80rpx;
	padding-top: 30rpx;
}
.fizeContent{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	padding-left: 20rpx;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.fizeContent .img{
	 width: 100rpx;
	 height: 200rpx;
}
.fizeContent .title{
	 width: 75%;
	 white-space:nowrap;
	 overflow:hidden;
	 text-overflow:ellipsis;
	 margin-bottom: 20rpx;
	 margin-left: 40rpx;
}

